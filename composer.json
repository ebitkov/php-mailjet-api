{
  "name": "ebitkov/php-mailjet-api",
  "description": "OOP extension for the PHP Mailjet wrapper",
  "type": "library",
  "autoload": {
    "psr-4": {
      "ebitkov\\Mailjet\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ebitkov\\Mailjet\\Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "Emanuel Bitkov",
      "email": "github@bitkov.de"
    }
  ],
  "require": {
    "php": ">=8.1",
    "mailjet/mailjet-apiv3-php": "^1.6",
    "symfony/serializer": "^7.0",
    "symfony/property-access": "^7.0",
    "doctrine/collections": "^2.1",
    "symfony/options-resolver": "^7.0",
    "phpdocumentor/reflection-docblock": "^5.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.5",
    "dg/bypass-finals": "^1.5",
    "symfony/yaml": "^7.0",
    "symfony/finder": "^7.0",
    "phpstan/phpstan": "^1.10"
  },
  "extra": {
    "sort-packages": true
  }
}
